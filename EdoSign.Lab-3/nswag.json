{
  "runtime": "Net90",
  "defaultVariables": null,
  "documentGenerator": {
    "fromDocument": {
      "url": "https://localhost:7275/swagger/v1/swagger.json",
      "output": "openapi.json",
      "newLineBehavior": "Auto"
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClient": {
      "className": "ApiClient",
      "moduleName": "",
      "namespace": "",
      "typeScriptVersion": 4.3,
      "template": "Fetch",

      "generateClientClasses": true,
      "generateDtoTypes": true,
      "typeStyle": "Class",
      "enumStyle": "Enum",
      "exportTypes": true,
      "markOptionalProperties": true,
      "generateDefaultValues": true,

      "operationGenerationMode": "MultipleClientsFromOperationId",

      "output": "GeneratedApiClient/apiClient.ts",
      "newLineBehavior": "Auto"
    }
  }
}

